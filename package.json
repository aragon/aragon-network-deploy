  "license": "GPL-3.0",
  "scripts": {
    "test": "./node_modules/@aragon/test-helpers/ganache-cli.sh",
    "deploy:dai": "node ./bin/index.js deploy-minime -c ./test/data/input/dai.js --verbose",
    "deploy:ant": "node ./bin/index.js deploy-minime -c ./test/data/input/ant.js --verbose",
    "deploy:anj": "node ./bin/index.js deploy-minime -c ./data/input/anj.js --verbose",
    "deploy:court": "node ./bin/index.js deploy-court --verbose",
    "deploy:faucet": "node ./bin/index.js deploy-faucet --verbose",
    "deploy:template": "node ./bin/index.js deploy-template --verbose",
    "deploy:wrapper-presale": "node ./bin/index.js deploy-wrapper-presale --verbose",
    "deploy:wrapper-bonding": "node ./bin/index.js deploy-wrapper-bonding --verbose",
    "upgrade:court": "node ./bin/index.js upgrade-court --verbose",
    "delay:court": "node ./bin/index.js delay-court --verbose",
    "migrate:registry": "node ./bin/index.js migrate-registry --verbose",
    "set:modules": "node ./bin/index.js set-modules --verbose",
    "set:subscriptions-params": "node ./bin/index.js set-subscriptions-params --verbose"
