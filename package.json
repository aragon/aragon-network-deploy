{
  "name": "@aragon/court-deploy",
  "version": "1.1.1",
  "description": "Aragon Court deployment scripts",
  "author": "Aragon Association",
  "license": "GPL-3.0",
  "scripts": {
    "test": "./node_modules/@aragon/test-helpers/ganache-cli.sh",
    "deploy:dai:rpc": "node ./bin/index.js minime -n rpc -c ./test/data/config/dai.js",
    "deploy:dai:staging": "node ./bin/index.js minime -n staging -c ./test/data/config/dai.js",
    "deploy:dai:ropsten": "node ./bin/index.js minime -n ropsten -c ./test/data/config/dai.js",
    "deploy:dai:rinkeby": "node ./bin/index.js minime -n rinkeby -c ./test/data/config/dai.js",
    "deploy:ant:rpc": "node ./bin/index.js minime -n rpc -c ./test/data/config/ant.js",
    "deploy:ant:staging": "node ./bin/index.js minime -n staging -c ./test/data/config/ant.js",
    "deploy:ant:ropsten": "node ./bin/index.js minime -n ropsten -c ./test/data/config/ant.js",
    "deploy:ant:rinkeby": "node ./bin/index.js minime -n rinkeby -c ./test/data/config/ant.js",
    "deploy:anj:rpc": "node ./bin/index.js minime -n rpc -c ./data/config/anj.js",
    "deploy:anj:staging": "node ./bin/index.js minime -n staging -c ./data/config/anj.js",
    "deploy:anj:ropsten": "node ./bin/index.js minime -n ropsten -c ./data/config/anj.js",
    "deploy:anj:rinkeby": "node ./bin/index.js minime -n rinkeby -c ./data/config/anj.js",
    "deploy:anj:mainnet": "node ./bin/index.js minime -n mainnet -c ./data/config/anj.js",
    "deploy:court:rpc": "rm -f data/output/minime.rpc.json && rm -f data/output/court.rpc.json && npm run deploy:dai:rpc && npm run deploy:anj:rpc && node ./bin/index.js court -n rpc",
    "deploy:court:staging": "node ./bin/index.js court -n staging",
    "deploy:court:ropsten": "node ./bin/index.js court -n ropsten",
    "deploy:court:rinkeby": "node ./bin/index.js court -n rinkeby",
    "deploy:court:mainnet": "node ./bin/index.js court -n mainnet",
    "upgrade:court:rpc": "node ./bin/index.js court-upgrade -n rpc",
    "upgrade:court:staging": "node ./bin/index.js court-upgrade -n stagingn",
    "upgrade:court:ropsten": "node ./bin/index.js court-upgrade -n ropsten",
    "upgrade:court:rinkeby": "node ./bin/index.js court-upgrade -n rinkeby",
    "upgrade:court:mainnet": "node ./bin/index.js court-upgrade -n mainnet",
    "deploy:presale:rpc": "rm -f data/output/minime.rpc.json data/output/presale.rpc.json && npm run deploy:ant:rpc && npm run deploy:anj:rpc && node ./bin/index.js presale --instance -n rpc",
    "deploy:presale:staging": "node ./bin/index.js presale -n staging --evmScript",
    "deploy:presale:rinkeby": "node ./bin/index.js presale -n rinkeby --evmScript",
    "deploy:presale:mainnet": "node ./bin/index.js presale -n mainnet --evmScript",
    "print:presale:ropsten": "node ./bin/index.js presale -n ropsten --print",
    "print:presale:staging": "node ./bin/index.js presale -n staging --print",
    "print:presale:rinkeby": "node ./bin/index.js presale -n rinkeby --print",
    "print:presale:mainnet": "node ./bin/index.js presale -n mainnet --print",
    "deploy:wrapper:ropsten": "node ./bin/index.js presale -n ropsten --wrapper",
    "deploy:wrapper:staging": "node ./bin/index.js presale -n staging --wrapper",
    "deploy:wrapper:rinkeby": "node ./bin/index.js presale -n rinkeby --wrapper",
    "deploy:wrapper:mainnet": "node ./bin/index.js presale -n mainnet --wrapper",
    "deploy:faucet:staging": "node ./bin/index.js faucet -n staging",
    "deploy:faucet:ropsten": "node ./bin/index.js faucet -n ropsten",
    "deploy:faucet:rinkeby": "node ./bin/index.js faucet -n rinkeby",
    "deploy:faucet:mainnet": "node ./bin/index.js faucet -n mainnet"
  },
  "dependencies": {
    "@aragon/apps-agent": "^2.0.0",
    "@aragon/apps-token-manager": "^2.1.0",
    "@aragon/apps-voting": "^2.1.0",
    "@aragon/erc20-faucet": "https://github.com/aragonone/erc20-faucet",
    "@aragon/court": "1.1.0",
    "@aragon/court-presale-activate": "1.1.0",
    "@aragon/minime": "^1.0.0",
    "@aragon/templates-externally-owned-presale-bonding-curve": "1.1.0",
    "@aragon/templates-shared": "^1.0.0",
    "@aragon/truffle-config-v5": "^1.0.0",
    "@truffle/config": "^1.1.21",
    "@truffle/contract": "^4.0.32",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "truffle-flattener": "^1.4.2",
    "web3": "^1.2.1",
    "web3-eth-abi": "^1.2.4",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@aragon/test-helpers": "^2.1.0",
    "ganache-cli": "^6.4.5",
    "truffle": "^5.0.34"
  }
}
